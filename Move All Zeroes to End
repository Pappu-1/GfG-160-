

// User function Template for Java

class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int n = arr.length;
        int j =0,count =0;
        for(int i=0;i<n;i++){
            if(arr[i] != 0){
                arr[j] = arr[i];
                j++;
            }else{
                count++;
            }
        }
        while(count-- > 0){
            arr[j++] = 0;
        }
    }
}
